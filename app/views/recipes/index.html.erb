<h1>All recipes</h1>
<br>
<table>
<tr>
  <th>Title</th>
  <th>Description</th>
  <th>Instruction</th>
</tr>
<tr>
  <% @recipes.each do |recipe| %>
      <td><%= link_to recipe.title, recipe_path(recipe) %></td>
        <td><%= recipe.description %></td>
          <td><%= recipe.instruction %></td>
  </tr>
  <% end %>
</table>
